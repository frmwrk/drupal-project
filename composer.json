{
    "name": "frmwrk/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "FRMWRK",
            "role": "Development"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.8.0",
        "drupal/core-composer-scaffold": "^8.8.0",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.5",
        "drupal/advagg": "^4.0",
        "drupal/block_blacklist": "^1.0",
        "drupal/config_filter": "^1.4",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.4",
        "drupal/devel": "^2.1",
        "drupal/devel_php": "^1.1",
        "drupal/field_tools": "^1.0@alpha",
        "drupal/flood_unblock": "^1.5",
        "drupal/honeypot": "^1.29",
        "drupal/hook_event_dispatcher": "^1.25",
        "drupal/image_widget_crop": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/layout_builder_restrictions": "^2.1",
        "drupal/layout_library": "^1.0@beta",
        "drupal/linkit": "^4.3",
        "drupal/login_security": "^1.5",
        "drupal/metatag": "^1.8",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.4",
        "drupal/real_aes": "^2.2",
        "drupal/redirect": "^1.4",
        "drupal/restui": "^1.17",
        "drupal/scheduler": "^1.0",
        "drupal/seckit": "^1.2",
        "drupal/session_limit": "^1.0@beta",
        "drupal/simple_sitemap": "^3.2",
        "drupal/stage_file_proxy": "^1.0@beta",
        "drupal/swiftmailer": "^1.0@beta",
        "drupal/twig_tweak": "^2.3",
        "drupal/username_enumeration_prevention": "^1.0@beta",
        "drupal/webform": "^5.2",
        "drush/drush": "^9.7.1 | ^10.0.0",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8.0",
        "drupal/coder": "^8.3",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phpmd/phpmd": "^2.6",
        "phpro/grumphp": "^0.15.2",
        "sebastian/phpcpd": "^3.0",
        "sensiolabs/security-checker": "^5.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.2"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
